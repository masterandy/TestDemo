    <webLinks>
        <fullName>Resend_Antifake</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Resend Antifake</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

var AF_Status = &quot;{!Parts_Order__c.Anti_Fake_Interface_Status__c}&quot;;
var Part_Status = &quot;{!Parts_Order__c.Status__c}&quot;;
var Nafc = &quot;{!Parts_Order__c.Need_Anti_Fake_check__c}&quot;;
var Afr = &quot;{!Parts_Order__c.Anti_Fake_result__c}&quot;;
var Afs= &quot;{!Parts_Order__c.Anti_Fake_SN__c}&quot;;
var AF_ID = &quot;{!Parts_Order__c.Id}&quot;;
if(Part_Status == &apos;Recommand&apos; || Nafc == &apos;false&apos; || Afr == null || Afr == &apos;&apos;){
alert(&apos;Status is Recommand or Need Anti-Fake check is false or Anti-Fake result is null , So Can not Send Antifake !&apos;);
}else{
if(confirm(&apos;Are you sure to Resend Antifake?&apos;)) 
{ 
var result=sforce.apex.execute(&quot;CSC_ReSend_Anti_Fake&quot;,&quot;Resend&quot;,{partOrderId:AF_ID}); 
if(result==&apos;1&apos;) 
{ 
alert(&apos;Resend Antifake Successfully!&apos;); 
window.location.reload(); 
} 
else if(result==&apos;2&apos;) 
{ 
alert(&apos;Resend Antifake Failed!&apos;); 
window.location.reload(); 
}else
{ 
alert(&apos;Resend Antifake Exception!&apos;); 
}
}
}</url>
    </webLinks>
