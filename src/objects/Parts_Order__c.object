        <fullName>CheckCatchAnalysisDescription</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Catch_Analysis__c,true,false) &amp;&amp;(ISNULL(Catch_Analysis_Description__c)|| ISBLANK( Catch_Analysis_Description__c ))</errorConditionFormula>
        <errorDisplayField>Catch_Analysis_Description__c</errorDisplayField>
        <errorMessage>Please input Catch Analysis Description!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckDoaDescription</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Parts_Doa__c , &apos;其他&apos;) &amp;&amp; (ISNULL(DOA_Description__c )|| ISBLANK( DOA_Description__c ))</errorConditionFormula>
        <errorDisplayField>DOA_Description__c</errorDisplayField>
        <errorMessage>Please input DOA Description!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckPartsDOA</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( X_Failure_Cd__c,&apos;55 - DOA/DFS&apos;)&amp;&amp;  
ISPICKVAL( Parts_Doa__c ,&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Parts_Doa__c</errorDisplayField>
        <errorMessage>Please select  Parts Doa!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckWorkOrderIsClose</fullName>
        <active>true</active>
        <description>更新辩假结果时需要检查work order 是否已关闭。如果关闭，则禁止修改</description>
        <errorConditionFormula>ISPICKVAL(Order__r.Work_Order_Name__r.Status, &apos;Closed&apos;)&amp;&amp;(ISCHANGED(Anti_Fake_SN__c)||ISCHANGED(Anti_Fake_result__c)||ISCHANGED(Need_Anti_Fake_check__c) )</errorConditionFormula>
        <errorMessage>The workorder is closed so cannot modify !</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Anti_Fake_SN</fullName>
        <active>true</active>
        <description>更新Antifake结果时，Anti fake SN 为必填</description>
        <errorConditionFormula>(DATEVALUE(CreatedDate) &gt; DATE(2018,01,24)) 
&amp;&amp;( Need_Anti_Fake_check__c = true ) 
&amp;&amp;!ISPICKVAL(Anti_Fake_result__c, &apos;&apos;) 
&amp;&amp;( Anti_Fake_SN__c = &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Anti_Fake_result__c</errorDisplayField>
        <errorMessage>Please enter &quot;Anti Fake SN&quot; when anti-fake is done.</errorMessage>
    </validationRules>
