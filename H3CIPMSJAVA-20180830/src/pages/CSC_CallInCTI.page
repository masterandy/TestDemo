    // 49 表示 退出
    // 50 表示 暂停
    // 51 表示 工作
    // 52 表示 话后处理
    if (agentMode == 48) {

        if (loginCnt == 0) {
            logger.log('CTI登录成功');
            timedCount();
        }
        loginCnt++;
        agent.agentId = agentId;
        agent.deviceId = deviceId;
        clearTimeout(agent.loginTimer);
        agent.retryTimes = 0;
    } else if (agentMode == 49) {
        agent.agentId = '';
        agent.deviceId = '';
    }
</script>

