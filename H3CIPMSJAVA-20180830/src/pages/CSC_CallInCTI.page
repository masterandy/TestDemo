    if (uui) {

        //从CTI控件中取到pinCode
        var userD = uui.split("|");

        ani = userD[0];
        dnis = userD[1];
        //400,soho,办事处,大客户【0,1,2,3】//'86761999,88870813,88991616|87368712,4089,4189|86760001,057186760001|88991616,057188991616'
        var ctiAlltel = document.getElementById('ctiAlltel').value;
        logger.log('ctiAlltel :' + ctiAlltel);
        var ptype = backTypeByPhone(ctiAlltel, dnis);
        var ajhm = userD[3];
        //86763031|88991616|00571143281505370538|1-1||3454||
        if (ptype==0&&ajhm.indexOf('3-4')==0||ptype == 3 && ajhm.indexOf('1')==0) {
            pinCode = userD[5];
        }
        if(ptype==0&&ajhm.indexOf('3-0')==0||ptype==2&&ajhm.indexOf('0-0')==0||ptype==3&&ajhm.indexOf('0')==0){
            customWorkOrderId = userD[6];
